      	allow_undefined_flag_GCJ='-flat_namespace -undefined suppress'
      else
        case ${MACOSX_DEPLOYMENT_TARGET} in
          10.[012])
            allow_undefined_flag_GCJ='-flat_namespace -undefined suppress'
            ;;
          10.*)
            allow_undefined_flag_GCJ='-undefined dynamic_lookup'
            ;;
        esac
      fi
	;;
      esac
