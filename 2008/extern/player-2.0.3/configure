rm -f conftest.err conftest.$ac_ext
echo "$as_me:$LINENO: result: $ac_header_preproc" >&5
echo "${ECHO_T}$ac_header_preproc" >&6

# So?  What about this header?
case $ac_header_compiler:$ac_header_preproc:$ac_c_preproc_warn_flag in
  yes:no: )
    { echo "$as_me:$LINENO: WARNING: $header: accepted by the compiler, rejected by the preprocessor!" >&5
echo "$as_me: WARNING: $header: accepted by the compiler, rejected by the preprocessor!" >&2;}
    { echo "$as_me:$LINENO: WARNING: $header: proceeding with the compiler's result" >&5
echo "$as_me: WARNING: $header: proceeding with the compiler's result" >&2;}
    ac_header_preproc=yes
    ;;
  no:yes:* )
    { echo "$as_me:$LINENO: WARNING: $header: present but cannot be compiled" >&5
echo "$as_me: WARNING: $header: present but cannot be compiled" >&2;}
    { echo "$as_me:$LINENO: WARNING: $header:     check for missing prerequisite headers?" >&5
echo "$as_me: WARNING: $header:     check for missing prerequisite headers?" >&2;}
    { echo "$as_me:$LINENO: WARNING: $header: see the Autoconf documentation" >&5
echo "$as_me: WARNING: $header: see the Autoconf documentation" >&2;}
    { echo "$as_me:$LINENO: WARNING: $header:     section \"Present But Cannot Be Compiled\"" >&5
echo "$as_me: WARNING: $header:     section \"Present But Cannot Be Compiled\"" >&2;}
    { echo "$as_me:$LINENO: WARNING: $header: proceeding with the preprocessor's result" >&5
echo "$as_me: WARNING: $header: proceeding with the preprocessor's result" >&2;}
    { echo "$as_me:$LINENO: WARNING: $header: in the future, the compiler will take precedence" >&5
echo "$as_me: WARNING: $header: in the future, the compiler will take precedence" >&2;}
